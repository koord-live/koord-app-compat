<?xml version="1.0" encoding="utf-8"?>
<Package xmlns="http://schemas.microsoft.com/appx/2010/manifest">
  <Identity Name="Koord.KoordRT" 
            Publisher="CN=2796F390-26AF-4354-BBB9-BD8D6E09CB7C"
            Version="4.0.4.0"
            ProcessorArchitecture="x64" />
  <Properties>
    <DisplayName>Koord</DisplayName>
    <PublisherDisplayName>Koord</PublisherDisplayName>
    <Description>Play live online in real-time with other musicians</Description>
    <Logo>mainicon.png</Logo>
  </Properties>
  <Prerequisites>
    <OSMinVersion></OSMinVersion>
    <OSMaxVersionTested></OSMaxVersionTested>
  </Prerequisites>
  <Resources>
    <Resource Language="en-us" />
  </Resources>
  <Applications>
    <Application Id="KOORD" Executable="Koord\Koord.exe" EntryPoint="Windows.FullTrustApplication">
      <VisualElements BackgroundColor="transparent" DisplayName="Koord" Logo="mainicon.png" 
                      SmallLogo="mainicon_small.png" ForegroundText="dark">
        <SplashScreen Image="krt_logo.png" />      
      </VisualElements>
      <Extensions>
        <Extension Category="windows.protocol">
          <Protocol Name="koord" />
        </Extension>
      </Extensions>
    </Application>
    <Application Id="KOORDASIOCONTROL" Executable="Koord\KoordASIOControl.exe" EntryPoint="Windows.FullTrustApplication">
      <VisualElements BackgroundColor="transparent" DisplayName="KoordASIO Control" Logo="koordasioicon.png"
                      SmallLogo="koordasioicon.png" ForegroundText="dark">
      </VisualElements>
    </Application>
  </Applications>
</Package>